{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strictFunctionTypes": true,
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
