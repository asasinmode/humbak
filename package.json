{
	"name": "humbak",
	"type": "module",
	"private": true,
	"packageManager": "pnpm@10.6.5",
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint": "eslint . --fix",
		"tsc": "nuxt typecheck",
